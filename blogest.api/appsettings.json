{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Seq":{
    "Url":"http://localhost:5341"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "MinimumLevel": "Information",
    "WriteTo":[
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  }
}
